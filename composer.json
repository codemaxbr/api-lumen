{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mongodb": "*",
        "ext-redis": "*",
        "albertcht/lumen-helpers": "^0.8.0",
        "browner12/helpers": "^2.1",
        "firebase/php-jwt": "^5.2",
        "flipbox/lumen-generator": "^8.0",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.2",
        "illuminate/notifications": "^5.7",
        "illuminate/redis": "5.7.x-dev",
        "illuminate/support": "^5.7",
        "jenssegers/mongodb": "^3.5",
        "laravel/lumen-framework": "5.7.*",
        "laravelista/lumen-vendor-publish": "^5.7.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "predis/predis": "^1.1",
        "prettus/l5-repository": "^2.7",
        "sentry/sentry-laravel": "^2.2",
        "spatie/laravel-permission": "^2.37",
        "swooletw/laravel-swoole": "^2.6",
        "vladimir-yuldashev/laravel-queue-rabbitmq": "^7.1.2",
        "vlucas/phpdotenv": "~2.2",
        "webpatser/laravel-uuid": "^3.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~7.0",
        "reliese/laravel": "^0.0.18"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Helpers/Main.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
